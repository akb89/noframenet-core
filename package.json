{
  "name": "noframenet-core",
  "version": "4.0.0beta",
  "description": "A set of core Mongoose models and utils for handling FrameNet data on a MongoDB database",
  "keywords": [
    "FrameNet",
    "MongoDB",
    "Mongoose"
  ],
  "author": "Alexandre Kabbach",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill './tests/**/*.@(js)' --reporter spec",
    "posttest": "npm run lint",
    "build": "./node_modules/.bin/babel --presets es2015,stage-0 src/ --out-dir lib/",
    "prepare": "npm run test && rm -rf lib/ && npm run build"
  },
  "homepage": "https://github.com/akb89/noframenet-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://akb89@github.com/akb89/noframenet-core.git"
  },
  "bugs": {
    "url": "https://github.com/akb89/noframenet-core/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "mongoose": "^4.10.7"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.4.2"
  }
}
